{
   "globals": {
      "document": true,
      "fetch": false
   },
   "parser": "babel-eslint",
   "rules": {
      "react/jsx-indent": ["error", 3],
      "react/jsx-indent-props": ["error", 3],
      "no-underscore-dangle": [0]
   },
   "extends": [
      "airbnb",
      "plugin:prettier/recommended",
      "plugin:react/recommended"
   ]
}
