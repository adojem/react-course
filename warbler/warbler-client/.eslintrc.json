{
   "env": {
      "browser": true
   },
   "rules": {
      "indent": ["error", 3, { "SwitchCase": 1}],
      "react/jsx-indent": ["error", 3],
      "no-underscore-dangle": "off",
      "react/jsx-filename-extension": "off",
      "implicit-arrow-linebreak": "off"
   },
   "extends": [
      "airbnb"
   ],
   "plugins": ["react"]
}
